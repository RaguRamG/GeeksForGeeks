/* insertion sort 2.*/

#include<stdio.h>
int main()
{
	int i,j,k,c,n,a[10000],temp;
	printf("\nEnter the no of elements:");
	scanf("%d",&n);
	for(i=0;i<n;i++)
	{
		scanf("%d",&a[i]);
	}
	for(i=n;i>=1;i--)
	{
		for(j=0;j<i;j++)
		{
			if(a[i]<a[j])
			{
				c=a[i];
				a[i]=a[j];
			}
			
		}
		for(k=0;k<n;k++)
			{
				printf("%d ",a[k]);
			}
		printf("\n");
	}
	
}	
		
